{
  "name": "@z-base/cvh",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "typescript",
    "z-base"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-base/cvh.git"
  },
  "bugs": {
    "url": "https://github.com/z-base/cvh/issues"
  },
  "homepage": "https://github.com/z-base/cvh#readme",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && node test/run-coverage.mjs && node test/e2e/run.mjs",
    "bench": "npm run build && node benchmark/bench.js",
    "prepublishOnly": "npm run test && npm run bench"
  },
  "c8": {
    "all": true,
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.d.ts",
      "test/**",
      "benchmark/**",
      "playwright.config.ts"
    ]
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.2.1",
    "c8": "^10.1.3",
    "fast-glob": "^3.3.3",
    "playwright": "^1.58.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@z-base/bytecodec": "^1.1.0",
    "@z-base/cryptosuite": "^1.0.1"
  }
}
